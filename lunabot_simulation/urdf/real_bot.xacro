      </geometry>
      <origin xyz="0 0 0" rpy="0 0 -3.14" />
    </collision>
  </link>

  <link name="d455_link">
    <visual>
      <geometry>
        <mesh filename="file:///$(find lunabot_simulation)/meshes/camera_link.stl" />
      </geometry>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <material name="camera_grey" />
    </visual>
    <collision>
      <geometry>
        <mesh filename="file:///$(find lunabot_simulation)/meshes/camera_link.stl" />
      </geometry>
      <origin xyz="0 0 0" rpy="0 0 0" />
    </collision>
  </link>

  <link name="d456_link">
    <visual>
      <geometry>
        <mesh filename="file:///$(find lunabot_simulation)/meshes/camera_link.stl" />
      </geometry>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <material name="camera_grey" />
    </visual>
    <collision>
      <geometry>
        <mesh filename="file:///$(find lunabot_simulation)/meshes/camera_link.stl" />
      </geometry>
      <origin xyz="0 0 0" rpy="0 0 0" />
    </collision>
  </link>

  <link name="front_right_wheel_link">
