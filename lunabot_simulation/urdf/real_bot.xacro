  </link>

  <link name="front_left_wheel_link">
    <visual>
      <geometry>
        <mesh filename="file:///$(find lunabot_simulation)/meshes/wheel_link.stl" />
      </geometry>
      <origin xyz="0 0 0" rpy="-1.5708 0 0" />
      <material name="black" />
    </visual>
    <inertial>
      <mass value="2.27" />
      <origin xyz="0 0 0" />
      <inertia ixx="0.0255" ixy="0.0" ixz="0.0" iyy="0.0255" iyz="0.0" izz="0.0510" />
    </inertial>
    <collision>
      <geometry>
        <mesh filename="file:///$(find lunabot_simulation)/meshes/wheel_link.stl" />
      </geometry>
      <origin xyz="0 0 0" rpy="-1.5708 0 0" />
    </collision>
  </link>

  <link name="rear_right_wheel_link">
    <visual>
      <geometry>
        <mesh filename="file:///$(find lunabot_simulation)/meshes/wheel_link.stl" />
      </geometry>
      <origin xyz="0 0 0" rpy="1.5708 0 0" />
      <material name="black" />
    </visual>
    <inertial>
      <mass value="2.27" />
      <origin xyz="0 0 0" />
      <inertia ixx="0.0255" ixy="0.0" ixz="0.0" iyy="0.0255" iyz="0.0" izz="0.0510" />
    </inertial>
    <collision>
      <geometry>
        <mesh filename="file:///$(find lunabot_simulation)/meshes/wheel_link.stl" />
      </geometry>
      <origin xyz="0 0 0" rpy="1.5708 0 0" />
    </collision>
  </link>

  <link name="rear_left_wheel_link">
    <visual>
